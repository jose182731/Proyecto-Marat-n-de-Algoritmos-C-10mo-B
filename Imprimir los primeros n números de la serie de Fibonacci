#include <iostream>
using namespace std;

int main() {
    int n, a = 0, b = 1, temp;
    cout << "Ingrese cantidad de tÃ©rminos: ";
    cin >> n;

    cout << "Serie de Fibonacci: ";
    for(int i = 1; i <= n; i++) {
        cout << a << " ";
        temp = a + b;
        a = b;
        b = temp;
    }
    cout << endl;
    return 0;
}
